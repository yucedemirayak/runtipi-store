{
  "services": [
    {
      "name": "reactive-resume",
      "image": "amruthpillai/reactive-resume:v5.0.5",
      "isMain": true,
      "internalPort": 3000,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "NODE_ENV",
          "value": "production"
        },
        {
          "key": "APP_URL",
          "value": "${TIPI_REACTIVE_RESUME_PUBLIC_URL}"
        },
        {
          "key": "PRINTER_APP_URL",
          "value": "http://reactive-resume:3000"
        },
        {
          "key": "PRINTER_ENDPOINT",
          "value": "${TIPI_REACTIVE_RESUME_CHROME_URL}?token=${TIPI_REACTIVE_RESUME_CHROME_TOKEN}"
        },
        {
          "key": "DATABASE_URL",
          "value": "${TIPI_REACTIVE_RESUME_DATABASE_URL}"
        },
        {
          "key": "AUTH_SECRET",
          "value": "${TIPI_REACTIVE_RESUME_ACCESS_TOKEN_SECRET}"
        },
        {
          "key": "SMTP_FROM",
          "value": "${TIPI_REACTIVE_RESUME_MAIL_FROM}"
        },
        {
          "key": "S3_ENDPOINT",
          "value": "http://${TIPI_REACTIVE_RESUME_STORAGE_ENDPOINT}:${TIPI_REACTIVE_RESUME_STORAGE_PORT}"
        },
        {
          "key": "S3_REGION",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_REGION}"
        },
        {
          "key": "S3_BUCKET",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_BUCKET}"
        },
        {
          "key": "S3_ACCESS_KEY_ID",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_ACCESS_KEY}"
        },
        {
          "key": "S3_SECRET_ACCESS_KEY",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_SECRET_KEY}"
        },
        {
          "key": "S3_FORCE_PATH_STYLE",
          "value": "true"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/data"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
