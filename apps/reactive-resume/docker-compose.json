{
  "services": [
    {
      "name": "reactive-resume",
      "image": "amruthpillai/reactive-resume:v5.0.5",
      "isMain": true,
      "internalPort": 3000,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "PORT",
          "value": "3000"
        },
        {
          "key": "NODE_ENV",
          "value": "production"
        },
        {
          "key": "PUBLIC_URL",
          "value": "${TIPI_REACTIVE_RESUME_PUBLIC_URL}"
        },
        {
          "key": "STORAGE_URL",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_URL}"
        },
        {
          "key": "CHROME_TOKEN",
          "value": "${TIPI_REACTIVE_RESUME_CHROME_TOKEN}"
        },
        {
          "key": "CHROME_URL",
          "value": "${TIPI_REACTIVE_RESUME_CHROME_URL}"
        },
        {
          "key": "DATABASE_URL",
          "value": "${TIPI_REACTIVE_RESUME_DATABASE_URL}"
        },
        {
          "key": "ACCESS_TOKEN_SECRET",
          "value": "${TIPI_REACTIVE_RESUME_ACCESS_TOKEN_SECRET}"
        },
        {
          "key": "REFRESH_TOKEN_SECRET",
          "value": "${TIPI_REACTIVE_RESUME_REFRESH_TOKEN_SECRET}"
        },
        {
          "key": "MAIL_FROM",
          "value": "${TIPI_REACTIVE_RESUME_MAIL_FROM}"
        },
        {
          "key": "STORAGE_ENDPOINT",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_ENDPOINT}"
        },
        {
          "key": "STORAGE_PORT",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_PORT}"
        },
        {
          "key": "STORAGE_REGION",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_REGION}"
        },
        {
          "key": "STORAGE_BUCKET",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_BUCKET}"
        },
        {
          "key": "STORAGE_ACCESS_KEY",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_ACCESS_KEY}"
        },
        {
          "key": "STORAGE_SECRET_KEY",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_SECRET_KEY}"
        },
        {
          "key": "STORAGE_USE_SSL",
          "value": "${TIPI_REACTIVE_RESUME_STORAGE_USE_SSL}"
        },
        {
          "key": "REDIS_URL",
          "value": "${TIPI_REACTIVE_RESUME_REDIS_URL}"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
