{
  "services": [
    {
      "name": "cloudbeaver",
      "image": "dbeaver/cloudbeaver:latest",
      "isMain": true,
      "internalPort": 8978,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/opt/cloudbeaver/workspace"
        }
      ]
    }
  ],
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json"
}
